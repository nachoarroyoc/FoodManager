<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".verUsuario"
    android:background="@drawable/wallpaper">

    <FrameLayout
        android:layout_width="359dp"
        android:layout_height="408dp"
        android:background="#C6FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.677">

        <TextView
            android:id="@+id/EmailAddress"
            android:layout_width="265dp"
            android:layout_height="49dp"
            android:layout_marginLeft="35dp"
            android:layout_marginTop="60dp"
            android:background="#00FFFFFF"
            android:backgroundTint="@android:color/white"
            android:ems="10"
            android:hint="email"
            android:inputType="textEmailAddress"
            android:textColor="#9F03DAC5"
            android:textColorHint="#9F03DAC5"
            android:textSize="20dp" />

        <LinearLayout
            android:layout_width="265dp"
            android:layout_height="1dp"
            android:layout_marginLeft="35dp"
            android:layout_marginTop="95dp"
            android:background="#9F03DAC5"
            android:orientation="horizontal"></LinearLayout>

        <TextView
            android:id="@+id/identificador"
            android:layout_width="265dp"
            android:layout_height="49dp"
            android:layout_marginLeft="35dp"
            android:layout_marginTop="140dp"
            android:background="#00FFFFFF"
            android:backgroundTint="@android:color/white"
            android:ems="10"
            android:hint="identificador"
            android:textColor="#9F03DAC5"
            android:textColorHint="#9F03DAC5"
            android:textSize="20dp" />

        <LinearLayout
            android:layout_width="265dp"
            android:layout_height="1dp"
            android:layout_marginLeft="35dp"
            android:layout_marginTop="175dp"
            android:background="#9F03DAC5"
            android:orientation="horizontal"></LinearLayout>

        <TextView
            android:id="@+id/Nombre"
            android:layout_width="265dp"
            android:layout_height="49dp"
            android:layout_marginLeft="35dp"
            android:layout_marginTop="220dp"
            android:background="#00FFFFFF"
            android:backgroundTint="@android:color/white"
            android:ems="10"
            android:hint="nombre"
            android:textColor="#9F03DAC5"
            android:textColorHint="#9F03DAC5"
            android:textSize="20dp" />

        <LinearLayout
            android:layout_width="265dp"
            android:layout_height="1dp"
            android:layout_marginLeft="35dp"
            android:layout_marginTop="255dp"
            android:background="#9F03DAC5"
            android:orientation="horizontal"></LinearLayout>

        <TextView
            android:id="@+id/Apellidos"
            android:layout_width="265dp"
            android:layout_height="49dp"
            android:layout_marginLeft="35dp"
            android:layout_marginTop="300dp"
            android:background="#00FFFFFF"
            android:backgroundTint="@android:color/white"
            android:ems="10"
            android:hint="apellidos"
            android:textColor="#9F03DAC5"
            android:textColorHint="#9F03DAC5"
            android:textSize="20dp" />

        <LinearLayout
            android:layout_width="265dp"
            android:layout_height="1dp"
            android:layout_marginLeft="35dp"
            android:layout_marginTop="335dp"
            android:background="#9F03DAC5"
            android:orientation="horizontal"></LinearLayout>


    </FrameLayout>

    <ImageView
        android:id="@+id/search18"
        android:layout_width="71dp"
        android:layout_height="62dp"
        android:onClick="returnToConfiguracion"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.047"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.023"
        app:srcCompat="@mipmap/atras" />
</androidx.constraintlayout.widget.ConstraintLayout>